{
    "name": "laraning/nova-surveyor",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laraning/boost": "*",
        "laraning/surveyor": "*"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\NovaSurveyor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\NovaSurveyor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Laraning",
            "email": "bruno.falcao@laraning.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "laraning/nova-surveyor",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laraning/boost": "*",
        "laravel/nova": "*",
        "laraning/surveyor": "*"
    },

    "autoload": {
        "psr-4": {
            "Laraning\\NovaSurveyor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\NovaSurveyor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
